div#producttiles .animate {
	z-index: 100;
	-webkit-perspective: 1000;
	-moz-perspective: 1000;
	perspective: 1000;
}
		div#producttiles .fadeout { visibility: hidden; }
		div#producttiles .animate div { box-shadow: 0 0 100px -20px black; }
div#producttiles div.selected div {
	-webkit-transform: rotateY(0deg) scale(1);
	-moz-transform: rotateY(0deg) scale(1);
	transform: rotateY(0deg) scale(1);
}
div#producttiles div.selected > a {
	-webkit-transform: rotateY(-180deg) scale(2);
	-moz-transform: rotateY(-180deg) scale(2);
	transform: rotateY(-180deg) scale(2);
}

div#producttiles div.leftmost div {
	-webkit-transform: translate(25%, 0) rotateY(0deg) scale(1);
